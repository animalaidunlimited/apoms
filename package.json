{
  "name": "apoms",
  "version": "0.0.0",
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:single-run": "ng test --watch=false",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "genpage": "node ./scripts/generatePage",
    "postgenpage": "ng lint --fix",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/apoms/stats.json",
    "configure:ci": "ts-node set-environment.ts",
    "app-config:ci": "ts-node set-app-yaml.ts",
    "build:ci": "ng build --prod --configuration=$NPM_CONFIG_ENVIRONMENT --sourceMap=false",
    "test:ci": "ng test --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCustom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s test:single-run format:fix format:check lint:fix lint:check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.6",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.14",
    "@angular/google-maps": "^11.2.6",
    "@angular/localize": "^11.2.14",
    "@angular/material": "^11.2.6",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@angular/service-worker": "^11.2.14",
    "@firebase/messaging": "^0.7.3",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/googlemaps": "^3.43.3",
    "@types/uuid": "^8.3.0",
    "ally.js": "^1.4.1",
    "angular2-uuid": "^1.1.1",
    "axobject-query": "^2.2.0",
    "canvasjs": "^1.8.3",
    "core-js": "^2.6.11",
    "cross-env": "^7.0.3",
    "firebase": "^8.6.7",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1102.13",
    "@angular-devkit/build-angular": "^12.0.4",
    "@angular-eslint/builder": "^2.0.0",
    "@angular-eslint/eslint-plugin": "^2.0.0",
    "@angular-eslint/eslint-plugin-template": "^2.0.0",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "^2.0.0",
    "@angular/cli": "^11.2.13",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@schematics/angular": "^11.2.6",
    "@types/hammerjs": "^2.0.39",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-react": "latest",
    "firebase-tools": "^9.9.0",
    "fuzzy": "^0.1.3",
    "husky": "^2.7.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "scripty": "^1.9.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
