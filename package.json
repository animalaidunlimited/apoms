{
  "name": "apoms",
  "version": "1.21.0",
  "engines": {
    "node": "16.x.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:single-run": "ng test --watch=false",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "genpage": "node ./scripts/generatePage",
    "postgenpage": "ng lint --fix",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/apoms/stats.json",
    "configure:ci": "ts-node set-environment.ts",
    "app-config:ci": "ts-node set-app-yaml.ts",
    "build:ci": "ng build --configuration production --configuration=$NPM_CONFIG_ENVIRONMENT --sourceMap=false",
    "test:ci": "ng test --watch=false --code-coverage --source-map true --browsers=ChromeHeadlessCustom"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^13.1.3",
    "@angular/animations": "^13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/fire": "^7.2.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.1.2",
    "@angular/google-maps": "^13.1.2",
    "@angular/localize": "^13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@angular/service-worker": "^13.1.2",
    "@firebase/messaging": "^0.7.15",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/dom-mediacapture-record": "^1.0.11",
    "@types/google.maps": "^3.47.2",
    "@types/uuid": "^8.3.4",
    "ally.js": "^1.4.1",
    "angular2-uuid": "^1.1.1",
    "axobject-query": "^2.2.0",
    "canvasjs": "^1.8.3",
    "core-js": "^2.6.12",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.3",
    "hammerjs": "^2.0.8",
    "ng-table-virtual-scroll": "^1.3.8",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1301.3",
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular-eslint/builder": "^2.1.1",
    "@angular-eslint/eslint-plugin": "^2.1.1",
    "@angular-eslint/eslint-plugin-template": "^2.1.1",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "^2.1.1",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@angular/language-service": "^13.1.2",
    "@schematics/angular": "^13.1.3",
    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "^3.6.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "4.16.1",
