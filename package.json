{
  "name": "apoms",
  "version": "0.0.0",
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:single-run": "ng test --watch=false",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "genpage": "node ./scripts/generatePage",
    "postgenpage": "ng lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s test:single-run format:fix format:check lint:fix lint:check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/fire": "^6.0.3",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^9.1.12",
    "@angular/google-maps": "9.1.3",
    "@angular/localize": "^9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@angular/service-worker": "^9.1.12",
    "@kolkov/ngx-gallery": "^1.2.3",
    "@types/googlemaps": "^3.39.2",
    "ally.js": "^1.4.1",
    "axobject-query": "^2.2.0",
    "canvasjs": "^1.8.3",
    "core-js": "^2.6.11",
    "firebase": "^7.23.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.900",
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "firebase-tools": "^8.12.1",
    "fuzzy": "^0.1.3",
    "husky": "^2.7.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "^5.4.4",
    "scripty": "^1.9.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
